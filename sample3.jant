START hello3;

FUNC calc(Ref a as Integer){
Integer num;
num = a +3;
return num;
};

MAIN 
{	
	Integer i,j,k;

	i = 7;
	j = 1 + 1;
	k = 7 * (j + 5);
	
	WHEN(j == 2)
	{
	    j = 6;
	}
	WHENIF(j == 1)
	{
	    j = 5;
	}
	OTHERWISE
	{
	    j = 4;
	};
	
	LOOP(i!=1)
	{
		i = i - 1;
	};
	
	PrintTxt("Hello World");
	
	i = calc(k);
	PrintStr("The value of i is %i. \n",i);
}

TERMINATE

